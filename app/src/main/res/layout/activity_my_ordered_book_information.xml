<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearLayout_Marriage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:context=".MyOrderedThemeMarriageInformationActivity"
    android:orientation="vertical"
    >

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tablayoutMarriage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:tabSelectedTextColor="@color/blue"
        app:tabTextColor="@android:color/black">

        <com.google.android.material.tabs.TabItem
            android:id="@+id/contact_marriage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:icon="@android:drawable/ic_menu_call" />

        <com.google.android.material.tabs.TabItem
            android:id="@+id/gallery_marriage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:icon="@android:drawable/ic_menu_gallery"
            />

        <com.google.android.material.tabs.TabItem
            android:id="@+id/maindata_marriage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:icon="@android:drawable/sym_contact_card"
            />

        <com.google.android.material.tabs.TabItem
            android:id="@+id/meeting_point_marriage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:icon="@drawable/ic_baseline_my_location_24"
             />

        <com.google.android.material.tabs.TabItem
            android:id="@+id/parents_marriage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:icon="@android:drawable/star_on"
            />

        <com.google.android.material.tabs.TabItem
            android:id="@+id/testimonials_marriage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:icon="@drawable/dust"
            />


    </com.google.android.material.tabs.TabLayout>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewPager_Marriage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>
